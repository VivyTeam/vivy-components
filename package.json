{
  "name": "vivy-components",
  "version": "0.0.5",
  "private": true,
  "scripts": {
    "linter:format":
      "prettier --trailing-comma es5 --single-quote --write '{,!(node_modules|.git|storybook-static)/**/}*.{js,json,jsx,md}'",
    "linter:check": "eslint stories/**/* && eslint src/**/*",
    "test": "jest",
    "test:watch": "jest --watch",
    "storybook": "start-storybook -p 3006 -s ./public",
    "build:storybook": "build-storybook -o ./dist/web",
    "build:package": "babel --presets react ./src --out-dir ./dist/build",
    "release:test": "release-it --dry-run",
    "release": "release-it"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./src/setupTests.js"
  },
  "dependencies": {
    "@storybook/addon-actions": "^3.4.6",
    "@storybook/addon-console": "^1.0.0",
    "@storybook/addon-info": "^3.4.7",
    "@storybook/addon-knobs": "^3.4.7",
    "@storybook/addon-links": "^3.4.6",
    "@storybook/addon-options": "^3.4.6",
    "@storybook/addons": "^3.4.6",
    "@storybook/react": "^3.4.6",
    "babel-eslint": "^8.2.3",
    "copy-to-clipboard": "^3.0.8",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "html-webpack-plugin": "^3.2.0",
    "prettier": "^1.13.5",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "rx-connect": "^0.7.0",
    "rxjs": "^5.5.11",
    "storybook-addon-jsx": "^5.3.0",
    "styled-components": "^3.2.3",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^23.0.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-preset-stage-0": "^6.24.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.17.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-react": "^7.7.0",
    "jest": "^23.2.0",
    "react-test-renderer": "^16.4.0",
    "release-it": "^7.4.7",
    "sinon": "^6.1.0",
    "style-loader": "^0.20.1"
  }
}
