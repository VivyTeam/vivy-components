{
  "name": "@vivy/vivy-components",
  "version": "0.0.3",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git://github.com/UvitaTeam/vivy-components"
  },
  "license": "MIT",
  "scripts": {
    "linter:format": "prettier --trailing-comma es5 --single-quote --write '{,!(node_modules|.git|storybook-static)/**/}*.{js,json,jsx,md}'",
    "linter:check": "eslint stories/ && eslint src/",
    "storybook": "start-storybook -p 3006 -s ./public",
    "release": "node build.js",
    "release:test": "node build.js test",
    "build:storybook": "storybook-to-ghpages",
    "build:package": "babel --presets react ./src --out-dir ./dist/src && cp -R public ./dist/public && cp package.json README.md index.js ./dist && cd ./dist && npm publish --access public"
  },
  "dependencies": {
    "@storybook/addon-actions": "^3.4.6",
    "@storybook/addon-console": "^1.0.0",
    "@storybook/addon-info": "^3.4.10",
    "@storybook/addon-links": "^3.4.6",
    "@storybook/addon-options": "^3.4.6",
    "@storybook/addons": "^3.4.6",
    "@storybook/react": "^3.4.6",
    "babel-eslint": "^8.2.3",
    "copy-to-clipboard": "^3.0.8",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "html-webpack-plugin": "^3.2.0",
    "prettier": "^1.13.5",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "storybook-addon-jsx": "^5.3.0",
    "styled-components": "^3.2.3",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1"
  },
  "devDependencies": {
    "@storybook/storybook-deployer": "^2.3.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^23.0.1",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "css-loader": "^0.28.9",
    "eslint": "^4.17.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^1.1.11",
    "jest": "^23.0.1",
    "react-test-renderer": "^16.4.0",
    "style-loader": "^0.20.1"
  }
}
